{
    "configurations": [
        {
            "name": "STM32F407G-DISC1",
            "includePath": [
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc/Legacy",
                "${workspaceFolder}/Middlewares/FreeRTOS/include",
                "${workspaceFolder}/Middlewares/FreeRTOS/src/portable/GCC/ARM_CM4F",
                "${workspaceFolder}/Middlewares/u8g2/include",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F4xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include"
            ],
            "defines": [
                "DEBUG",
                "USE_HAL_DRIVER",
                "STM32F407xx"
            ],
            "compilerPath": "${env:ARM_NONE_EABI_HOME}/bin/arm-none-eabi-gcc.exe",
            "cStandard": "c17",
            "intelliSenseMode": "windows-gcc-arm",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard",
                "-Og",
                "-Wall",
                "-fdata-sections",
                "-ffunction-sections",
                "-std=gnu17",
                "-g",
                "-gdwarf-2"
            ],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}